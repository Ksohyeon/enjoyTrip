<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.place.model.mapper.PlaceMapper">

	<resultMap type="placeDto" id="place">
		<result column="placeno" property="placeno"/>
		<result column="user_id" property="user_id"/>
		<result column="lat" property="lat"/>
		<result column="lon" property="lon"/>
		<result column="title" property="title"/>
		<result column="content" property="content"/>
		<result column="hit" property="hit"/>
		<result column="date" property="date"/>
		<result column="created_at" property="created_at"/>
		<result column="image" property="image"/>
	</resultMap>
	
	<select id="placeList" resultMap="place">
		select placeno, user_id, lat, lon, title, content, hit, date, created_at, image
		from place;
	</select>
</mapper>